{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug TPGR",
      "program": "${workspaceFolder}/target/debug/import_tpgr",
      "args": [
        "./import/tpgr/example_data/small_road_network.tpgr",
        "./data/imported/tpgr/example_data/small_road_network"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Preprocess (grid)",
      "program": "${workspaceFolder}/target/debug/sumo-tdcch-preprocessor",
      "args": [
        "--input-dir",
        "./import/sumo/grid",
        "--input-prefix",
        "grid",
        "--output-dir",
        "./data/imported/sumo/grid",
        "--trips-file",
        "${workspaceFolder}/import/sumo/grid/grid.trips.xml"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${workspaceFolder}/lib/InertialFlowCutter/build:${env:PATH}"
      },
      "preLaunchTask": "Build (debug)"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Preprocess (example data)",
      "program": "${workspaceFolder}/target/debug/sumo-tdcch-preprocessor",
      "args": [
        "--input-dir",
        "./import/sumo/example-data",
        "--input-prefix",
        "example-data",
        "--output-dir",
        "./data/imported/sumo/example-data",
        "--trips-file",
        "${workspaceFolder}/import/sumo/example-data/example-data.trips.xml"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${workspaceFolder}/lib/InertialFlowCutter/build:${env:PATH}"
      },
      "preLaunchTask": "Build (debug)"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Router (example data)",
      "program": "${workspaceFolder}/target/debug/sumo-tdcch-router",
      "args": [
        "--input-dir",
        "${workspaceFolder}/data/imported/sumo/example-data",
        "--input-prefix",
        "example-data",
        "--iteration",
        "1"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${workspaceFolder}/lib/InertialFlowCutter/build:${env:PATH}"
      },
      "preLaunchTask": "Preprocess (example data)"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Router (grid)",
      "program": "${workspaceFolder}/target/debug/sumo-tdcch-router",
      "args": [
        "--input-dir",
        "./data/imported/sumo/grid",
        "--input-prefix",
        "grid",
        "--iteration",
        "1"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${workspaceFolder}/lib/InertialFlowCutter/build:${env:PATH}"
      },
      "preLaunchTask": "Build (debug)"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Router (Iteration 1)",
      "program": "${workspaceFolder}/target/debug/sumo-tdcch-router",
      "args": [
        "--input-dir",
        "${workspaceFolder}/../ma/data_reduced/karlsruhe-900/karlsruhe-900",
        "--input-prefix",
        "karlsruhe",
        "--iteration",
        "1"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${workspaceFolder}/lib/InertialFlowCutter/build:${env:PATH}",
        "RUST_BACKTRACE": "1"
      },
      "preLaunchTask": "Build (debug)"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Connection Counter (dbg)",
      "program": "${workspaceFolder}/target/debug/sumo-connections-counter",
      "args": [
        "--input-dir",
        "./import/sumo/karlsruhe-dbg",
        "--input-prefix",
        "karlsruhe"
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "Build (debug)"
    }
  ]
}
